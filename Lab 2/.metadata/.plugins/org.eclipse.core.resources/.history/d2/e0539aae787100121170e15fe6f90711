extern int fibonacci(int nn);

int fibEasy(int nn)
{
	if(nn == 1 || nn == 2)
	{
		return 1;
	}
	else if (nn < 1)
	{
		return 0;
	}
	else if(nn > 10)
	{
		return fibEasy(nn-1) - fibEasy(nn-2);
	}
	else if(nn <= 10)
	{
		return fibEasy(nn-1) + fibEasy(nn-2);
	}
	return 0;
}

int main(void) {

	int nn = 9;
	int fibOutEasy = fibEasy(nn);

	int fibOut = fibonacci(nn);

	//if(fibOutEasy != fibOut);

	return 0;
}
